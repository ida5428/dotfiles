[
   {
      "context": "Editor && vim_mode == insert",
      "bindings": {
         "j k": "vim::NormalBefore"
      }
   },
   {
      "context": "ProjectPanel",
      "bindings": {
         "shift-d": [
            "project_panel::Trash",
            {
               "skip_prompt": false
            }
         ]
      }
   },
   {
      "context": "Workspace",
      "bindings": {
         "cmd cmd": "project_panel::ToggleFocus"
      }
   },
   {
      "context": "Workspace",
      "bindings": {
         "shift shift": "file_finder::Toggle"
      }
   },
   {
   "context": "Workspace",
      "bindings": {
         "alt alt": "terminal_panel::ToggleFocus"
      }
   }
]
